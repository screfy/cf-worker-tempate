{
	"compilerOptions": {
		"moduleResolution": "Node",
		"target": "ES2019",
		"module": "CommonJS",
		"outDir": "dist",
		"lib": ["ES2019"],
		"types": ["@cloudflare/workers-types"],
		"noImplicitAny": true,
		"alwaysStrict": true,
		"skipLibCheck": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"esModuleInterop": true,
		"incremental": true,
		"allowJs": false
	},
	"include": ["src/**/*.ts"],
	"exclude": ["node_modules"]
}
